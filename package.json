{
  "name": "desafio-aeduca",
  "version": "1.0.0",
  "description": "Desafio Full-Stack A+ Educação",
  "main": "index.js",
  "scripts": {
    "backend": "NODE_ENV=development node --watch ./backend/server.js",
    "frontend": "vite --config ./frontend/vite.config.js",
    "build": "npm install && vite build --config ./frontend/vite.config.js",
    "start": "NODE_ENV=production node ./backend/server.js",
    "lint": "eslint --no-cache .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test:frontend": "vitest --config frontend/vite.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedrotrasfereti/desafio-aeduca.git"
  },
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "full-stack",
    "nodejs",
    "expressjs",
    "vuejs",
    "mysql"
  ],
  "author": "pedrotrasfereti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pedrotrasfereti/desafio-aeduca/issues"
  },
  "homepage": "https://github.com/pedrotrasfereti/desafio-aeduca#readme",
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.11.5",
    "pinia": "^2.3.0",
    "roboto-fontface": "^0.10.0",
    "vite": "^6.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "prettier": "3.4.2",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.82.0",
    "sass-embedded": "^1.82.0",
    "unplugin-fonts": "^1.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^2.1.8"
  }
}
